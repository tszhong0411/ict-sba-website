generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  displayName String?
  username    String   @unique
  email       String   @unique
  password    String
  image       String?
  createdAt   DateTime @default(now())
}
